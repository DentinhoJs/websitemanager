{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/control.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/status.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/control/(.*)",
      "dest": "/api/control.js"
    },
    {
      "src": "/api/status/(.*)",
      "dest": "/api/status.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}
